{
  "name": "degroet",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bertpard/degroet.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bertpard/degroet/issues"
  },
  "homepage": "https://github.com/bertpard/degroet#readme",
  "description": "",
  "clean": [
    "static/"
  ],
  "sass": {
    "files": [
      {
        "src": "sass/*.scss",
        "dest": "static/css/",
        "file": "main.css"
      }
    ],
    "hint": {
      "src": [
        "sass/**/*.scss",
        "!./sass/base/_normalize.scss"
      ]
    },
    "watch": "sass/**/*.scss",
    "autoprefixer": {
      "browsers": [
        "Firefox >=27",
        "Chrome >= 33",
        "ie >= 9",
        "Safari >= 7",
        "iOS >= 7",
        "Android >= 4.1"
      ]
    }
  },
  "img": {
    "src": "img/**/*",
    "dest": "static/img/",
    "watch": "img/**/*"
  },
  "devDependencies": {
    "del": "^1.1.1",
    "event-stream": "^3.3.2",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-bless": "3.0.1",
    "gulp-cached": "^1.0.4",
    "gulp-concat": "^2.5.2",
    "gulp-copy": "^0.0.2",
    "gulp-eslint": "^1.1.0",
    "gulp-jekyll": "0.0.0",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.0.0",
    "gulp-sass": "^2.1.0",
    "gulp-scss-lint": "^0.3.8",
    "gulp-sourcemaps": "^1.5.1",
    "gulp-uglify": "^1.1.0",
    "gulp-util": "^3.0.4",
    "gulp-watch": "^4.3.8",
    "node-sass": "^3.8.0"
  }
}
